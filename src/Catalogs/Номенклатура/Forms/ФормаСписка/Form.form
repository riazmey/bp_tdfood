    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ВЫРАЗИТЬ(ОстаткиНаСкладе.Субконто1 КАК Справочник.Номенклатура) КАК Номенклатура,
	СУММА(ЕСТЬNULL(ОстаткиНаСкладе.КоличествоОстаток, 0)) КАК КоличествоОстаток
ПОМЕСТИТЬ ВТ_ОстаткиНаСкладе
ИЗ
	РегистрБухгалтерии.Хозрасчетный.Остатки(
			&amp;ДатаОстатков,
			Счет В (&amp;СчетаЗапасов),
			&amp;ВидыСубконто,
			ВЫБОР
					КОГДА &amp;Организация &lt;> ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка)
						ТОГДА Организация = &amp;Организация
					ИНАЧЕ ИСТИНА
				КОНЕЦ
				И ВЫБОР
					КОГДА &amp;Склад &lt;> ЗНАЧЕНИЕ(Справочник.Склады.ПустаяСсылка)
						ТОГДА Субконто2 = &amp;Склад
					ИНАЧЕ ИСТИНА
				КОНЕЦ) КАК ОстаткиНаСкладе

СГРУППИРОВАТЬ ПО
	ВЫРАЗИТЬ(ОстаткиНаСкладе.Субконто1 КАК Справочник.Номенклатура)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Базар_СопроводительныеДокументы.Номенклатура КАК Номенклатура,
	Базар_СопроводительныеДокументы.СтандартНаПродукцию.ВидСтандартаНаПродукцию КАК ВидСтандартаНаПродукцию
ПОМЕСТИТЬ ВТ_СопроводительныеДокументы
ИЗ
	РегистрСведений.Базар_СопроводительныеДокументы КАК Базар_СопроводительныеДокументы
ГДЕ
	Базар_СопроводительныеДокументы.ДатаОкончанияДействия >= &amp;ДатаОстатков
	И Базар_СопроводительныеДокументы.Регистратор.Дата &lt;= &amp;ДатаОстатков

СГРУППИРОВАТЬ ПО
	Базар_СопроводительныеДокументы.Номенклатура,
	Базар_СопроводительныеДокументы.СтандартНаПродукцию.ВидСтандартаНаПродукцию
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_СопроводительныеДокументы.Номенклатура КАК Номенклатура,
	&quot;ГОСТ&quot; КАК ВидСтандартаНаПродукцию
ПОМЕСТИТЬ ГОСТ
ИЗ
	ВТ_СопроводительныеДокументы КАК ВТ_СопроводительныеДокументы
ГДЕ
	ВТ_СопроводительныеДокументы.ВидСтандартаНаПродукцию = ЗНАЧЕНИЕ(Перечисление.Базар_ВидыСтандартовНаПродукцию.ГОСТ)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_СопроводительныеДокументы.Номенклатура КАК Номенклатура,
	&quot;ГОСТ Р&quot; КАК ВидСтандартаНаПродукцию
ПОМЕСТИТЬ ГОСТР
ИЗ
	ВТ_СопроводительныеДокументы КАК ВТ_СопроводительныеДокументы
ГДЕ
	ВТ_СопроводительныеДокументы.ВидСтандартаНаПродукцию = ЗНАЧЕНИЕ(Перечисление.Базар_ВидыСтандартовНаПродукцию.ГОСТР)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_СопроводительныеДокументы.Номенклатура КАК Номенклатура,
	&quot;ТУ&quot; КАК ВидСтандартаНаПродукцию
ПОМЕСТИТЬ ТУ
ИЗ
	ВТ_СопроводительныеДокументы КАК ВТ_СопроводительныеДокументы
ГДЕ
	ВТ_СопроводительныеДокументы.ВидСтандартаНаПродукцию = ЗНАЧЕНИЕ(Перечисление.Базар_ВидыСтандартовНаПродукцию.ТУ)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_СопроводительныеДокументы.Номенклатура КАК Номенклатура,
	&quot;СТО&quot; КАК ВидСтандартаНаПродукцию
ПОМЕСТИТЬ СТО
ИЗ
	ВТ_СопроводительныеДокументы КАК ВТ_СопроводительныеДокументы
ГДЕ
	ВТ_СопроводительныеДокументы.ВидСтандартаНаПродукцию = ЗНАЧЕНИЕ(Перечисление.Базар_ВидыСтандартовНаПродукцию.СТО)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_СопроводительныеДокументы.Номенклатура КАК Номенклатура,
	&quot;СТБ&quot; КАК ВидСтандартаНаПродукцию
ПОМЕСТИТЬ СТБ
ИЗ
	ВТ_СопроводительныеДокументы КАК ВТ_СопроводительныеДокументы
ГДЕ
	ВТ_СопроводительныеДокументы.ВидСтандартаНаПродукцию = ЗНАЧЕНИЕ(Перечисление.Базар_ВидыСтандартовНаПродукцию.СТБ)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_СопроводительныеДокументы.Номенклатура КАК Номенклатура,
	&quot;ТРТС&quot; КАК ВидСтандартаНаПродукцию
ПОМЕСТИТЬ ТРТС
ИЗ
	ВТ_СопроводительныеДокументы КАК ВТ_СопроводительныеДокументы
ГДЕ
	ВТ_СопроводительныеДокументы.ВидСтандартаНаПродукцию = ЗНАЧЕНИЕ(Перечисление.Базар_ВидыСтандартовНаПродукцию.ТРТС)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_СопроводительныеДокументы.Номенклатура КАК Номенклатура,
	&quot;Импорт&quot; КАК ВидСтандартаНаПродукцию
ПОМЕСТИТЬ Импорт
ИЗ
	ВТ_СопроводительныеДокументы КАК ВТ_СопроводительныеДокументы
ГДЕ
	ВТ_СопроводительныеДокументы.ВидСтандартаНаПродукцию = ЗНАЧЕНИЕ(Перечисление.Базар_ВидыСтандартовНаПродукцию.Импорт)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЕСТЬNULL(ГОСТ_ГОСТР_ТУ_СТО.Номенклатура, СТБ_ТРТС_Импорт.Номенклатура) КАК Номенклатура,
	ВЫБОР
		КОГДА НЕ ГОСТ_ГОСТР_ТУ_СТО.ВидСтандартаНаПродукцию ЕСТЬ NULL
			ТОГДА ГОСТ_ГОСТР_ТУ_СТО.ВидСтандартаНаПродукцию
		ИНАЧЕ &quot;&quot;
	КОНЕЦ + ВЫБОР
		КОГДА НЕ СТБ_ТРТС_Импорт.ВидСтандартаНаПродукцию ЕСТЬ NULL
				И НЕ ГОСТ_ГОСТР_ТУ_СТО.ВидСтандартаНаПродукцию ЕСТЬ NULL
			ТОГДА &quot;, &quot;
		ИНАЧЕ &quot;&quot;
	КОНЕЦ + ВЫБОР
		КОГДА НЕ СТБ_ТРТС_Импорт.ВидСтандартаНаПродукцию ЕСТЬ NULL
			ТОГДА СТБ_ТРТС_Импорт.ВидСтандартаНаПродукцию
		ИНАЧЕ &quot;&quot;
	КОНЕЦ КАК ВидСтандартаНаПродукцию
ПОМЕСТИТЬ ВТ_КонкатенацияВидовСтандартов
ИЗ
	(ВЫБРАТЬ
		ЕСТЬNULL(ГОСТ_ГОСТР.Номенклатура, ТУ_СТО.Номенклатура) КАК Номенклатура,
		ВЫБОР
			КОГДА НЕ ГОСТ_ГОСТР.ВидСтандартаНаПродукцию ЕСТЬ NULL
				ТОГДА ГОСТ_ГОСТР.ВидСтандартаНаПродукцию
			ИНАЧЕ &quot;&quot;
		КОНЕЦ + ВЫБОР
			КОГДА НЕ ТУ_СТО.ВидСтандартаНаПродукцию ЕСТЬ NULL
					И НЕ ГОСТ_ГОСТР.ВидСтандартаНаПродукцию ЕСТЬ NULL
				ТОГДА &quot;, &quot;
			ИНАЧЕ &quot;&quot;
		КОНЕЦ + ВЫБОР
			КОГДА НЕ ТУ_СТО.ВидСтандартаНаПродукцию ЕСТЬ NULL
				ТОГДА ТУ_СТО.ВидСтандартаНаПродукцию
			ИНАЧЕ &quot;&quot;
		КОНЕЦ КАК ВидСтандартаНаПродукцию
	ИЗ
		(ВЫБРАТЬ
			ЕСТЬNULL(ГОСТ.Номенклатура, ГОСТР.Номенклатура) КАК Номенклатура,
			ВЫБОР
				КОГДА НЕ ГОСТ.ВидСтандартаНаПродукцию ЕСТЬ NULL
					ТОГДА ГОСТ.ВидСтандартаНаПродукцию
				ИНАЧЕ &quot;&quot;
			КОНЕЦ + ВЫБОР
				КОГДА НЕ ГОСТР.ВидСтандартаНаПродукцию ЕСТЬ NULL
						И НЕ ГОСТ.ВидСтандартаНаПродукцию ЕСТЬ NULL
					ТОГДА &quot;, &quot;
				ИНАЧЕ &quot;&quot;
			КОНЕЦ + ВЫБОР
				КОГДА НЕ ГОСТР.ВидСтандартаНаПродукцию ЕСТЬ NULL
					ТОГДА ГОСТР.ВидСтандартаНаПродукцию
				ИНАЧЕ &quot;&quot;
			КОНЕЦ КАК ВидСтандартаНаПродукцию
		ИЗ
			ГОСТ КАК ГОСТ
				ПОЛНОЕ СОЕДИНЕНИЕ ГОСТР КАК ГОСТР
				ПО ГОСТ.Номенклатура = ГОСТР.Номенклатура) КАК ГОСТ_ГОСТР
			ПОЛНОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
				ЕСТЬNULL(ТУ.Номенклатура, СТО.Номенклатура) КАК Номенклатура,
				ВЫБОР
					КОГДА НЕ ТУ.ВидСтандартаНаПродукцию ЕСТЬ NULL
						ТОГДА ТУ.ВидСтандартаНаПродукцию
					ИНАЧЕ &quot;&quot;
				КОНЕЦ + ВЫБОР
					КОГДА НЕ СТО.ВидСтандартаНаПродукцию ЕСТЬ NULL
							И НЕ ТУ.ВидСтандартаНаПродукцию ЕСТЬ NULL
						ТОГДА &quot;, &quot;
					ИНАЧЕ &quot;&quot;
				КОНЕЦ + ВЫБОР
					КОГДА НЕ СТО.ВидСтандартаНаПродукцию ЕСТЬ NULL
						ТОГДА СТО.ВидСтандартаНаПродукцию
					ИНАЧЕ &quot;&quot;
				КОНЕЦ КАК ВидСтандартаНаПродукцию
			ИЗ
				ТУ КАК ТУ
					ПОЛНОЕ СОЕДИНЕНИЕ СТО КАК СТО
					ПО ТУ.Номенклатура = СТО.Номенклатура) КАК ТУ_СТО
			ПО ГОСТ_ГОСТР.Номенклатура = ТУ_СТО.Номенклатура) КАК ГОСТ_ГОСТР_ТУ_СТО
		ПОЛНОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ЕСТЬNULL(СТБ_ТРТС.Номенклатура, Импорт.Номенклатура) КАК Номенклатура,
			ВЫБОР
				КОГДА НЕ СТБ_ТРТС.ВидСтандартаНаПродукцию ЕСТЬ NULL
					ТОГДА СТБ_ТРТС.ВидСтандартаНаПродукцию
				ИНАЧЕ &quot;&quot;
			КОНЕЦ + ВЫБОР
				КОГДА НЕ СТБ_ТРТС.ВидСтандартаНаПродукцию ЕСТЬ NULL
						И НЕ Импорт.ВидСтандартаНаПродукцию ЕСТЬ NULL
					ТОГДА &quot;, &quot;
				ИНАЧЕ &quot;&quot;
			КОНЕЦ + ВЫБОР
				КОГДА НЕ Импорт.ВидСтандартаНаПродукцию ЕСТЬ NULL
					ТОГДА Импорт.ВидСтандартаНаПродукцию
				ИНАЧЕ &quot;&quot;
			КОНЕЦ КАК ВидСтандартаНаПродукцию
		ИЗ
			(ВЫБРАТЬ
				ЕСТЬNULL(ТРТС.Номенклатура, СТБ.Номенклатура) КАК Номенклатура,
				ВЫБОР
					КОГДА НЕ ТРТС.ВидСтандартаНаПродукцию ЕСТЬ NULL
						ТОГДА ТРТС.ВидСтандартаНаПродукцию
					ИНАЧЕ &quot;&quot;
				КОНЕЦ + ВЫБОР
					КОГДА НЕ СТБ.ВидСтандартаНаПродукцию ЕСТЬ NULL
							И НЕ ТРТС.ВидСтандартаНаПродукцию ЕСТЬ NULL
						ТОГДА &quot;, &quot;
					ИНАЧЕ &quot;&quot;
				КОНЕЦ + ВЫБОР
					КОГДА НЕ СТБ.ВидСтандартаНаПродукцию ЕСТЬ NULL
						ТОГДА СТБ.ВидСтандартаНаПродукцию
					ИНАЧЕ &quot;&quot;
				КОНЕЦ КАК ВидСтандартаНаПродукцию
			ИЗ
				ТРТС КАК ТРТС
					ПОЛНОЕ СОЕДИНЕНИЕ СТБ КАК СТБ
					ПО ТРТС.Номенклатура = СТБ.Номенклатура) КАК СТБ_ТРТС
				ПОЛНОЕ СОЕДИНЕНИЕ Импорт КАК Импорт
				ПО СТБ_ТРТС.Номенклатура = Импорт.Номенклатура) КАК СТБ_ТРТС_Импорт
		ПО ГОСТ_ГОСТР_ТУ_СТО.Номенклатура = СТБ_ТРТС_Импорт.Номенклатура

СГРУППИРОВАТЬ ПО
	ЕСТЬNULL(ГОСТ_ГОСТР_ТУ_СТО.Номенклатура, СТБ_ТРТС_Импорт.Номенклатура),
	ВЫБОР
		КОГДА НЕ ГОСТ_ГОСТР_ТУ_СТО.ВидСтандартаНаПродукцию ЕСТЬ NULL
			ТОГДА ГОСТ_ГОСТР_ТУ_СТО.ВидСтандартаНаПродукцию
		ИНАЧЕ &quot;&quot;
	КОНЕЦ + ВЫБОР
		КОГДА НЕ СТБ_ТРТС_Импорт.ВидСтандартаНаПродукцию ЕСТЬ NULL
				И НЕ ГОСТ_ГОСТР_ТУ_СТО.ВидСтандартаНаПродукцию ЕСТЬ NULL
			ТОГДА &quot;, &quot;
		ИНАЧЕ &quot;&quot;
	КОНЕЦ + ВЫБОР
		КОГДА НЕ СТБ_ТРТС_Импорт.ВидСтандартаНаПродукцию ЕСТЬ NULL
			ТОГДА СТБ_ТРТС_Импорт.ВидСтандартаНаПродукцию
		ИНАЧЕ &quot;&quot;
	КОНЕЦ
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СправочникНоменклатура.Ссылка КАК Ссылка,
	СправочникНоменклатура.ПометкаУдаления КАК ПометкаУдаления,
	СправочникНоменклатура.Код КАК Код,
